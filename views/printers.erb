<div class="paper">
  <div class="content">
    <% if @printer %>
      <h1>Your printer</h1>
      <p>Type: <%= @printer.type %></p>
      <p>ID: <%= @printer.id %></p>
      <p>Print URL: <a href="<%= @print_url %>">here</a></p>
    <% else %>
      <p>Sorry, I haven't seen any printers connect from near you</p>
    <% end %>
  </div>
</div>